<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" minHeight="500.0" minWidth="953.0" prefHeight="700.0" prefWidth="953.0" userData="Exam Scheduler - classrooms" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ClassroomsController">
   <children>
      <HBox fillHeight="false" minHeight="-Infinity" prefHeight="45.0">
         <children>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#Back" prefHeight="37.0" prefWidth="55.0" text="Back">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_LEFT" fillHeight="false" minHeight="-Infinity" prefHeight="34.0">
         <children>
            <Button fx:id="NamOrd" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#NameOrder" prefHeight="28.0" prefWidth="81.0" text="Name \/">
               <font>
                  <Font size="16.0" />
               </font>
               <padding>
                  <Insets left="7.0" top="-1.0" />
               </padding>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="SizOrd" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#SizeOrder" prefHeight="28.0" prefWidth="58.0" text="Size">
               <font>
                  <Font size="16.0" />
               </font>
               <padding>
                  <Insets left="7.0" top="-1.0" />
               </padding>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="EquOrd" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#EquipmentOrder" prefHeight="28.0" prefWidth="114.0" text="Equipments">
               <font>
                  <Font size="16.0" />
               </font>
               <padding>
                  <Insets left="7.0" top="-1.0" />
               </padding>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
      <ListView fx:id="Rooms" prefHeight="310.0" prefWidth="481.0" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </ListView>
      <HBox alignment="CENTER_LEFT" minHeight="-Infinity" prefHeight="35.0" prefWidth="481.0">
         <children>
            <TextField fx:id="NamInp" minHeight="-Infinity" minWidth="-Infinity" onKeyTyped="#Name" prefHeight="30.0" prefWidth="81.0">
               <font>
                  <Font size="13.0" />
               </font>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="SizInp" minHeight="-Infinity" minWidth="-Infinity" onKeyTyped="#Size" prefHeight="30.0" prefWidth="58.0">
               <font>
                  <Font size="12.9" />
               </font>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="EquInp" prefHeight="30.0" prefWidth="557.0" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" minHeight="-Infinity" prefHeight="44.0" prefWidth="481.0">
         <children>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#AddRoom" prefHeight="37.0" prefWidth="270.0" text="Add new / overwrite existing room">
               <font>
                  <Font size="16.0" />
               </font>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
               <padding>
                  <Insets top="-1.0" />
               </padding>
            </Button>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#RemoveRoom" prefHeight="37.0" prefWidth="200.0" text="Remove room (by name)">
               <font>
                  <Font size="16.0" />
               </font>
               <padding>
                  <Insets top="-1.0" />
               </padding>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets left="20.0" right="20.0" />
   </padding>
</VBox>
